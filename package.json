{
  "name": "alkemy-node",
  "version": "1.0.0",
  "description": "Intento del Proyecto de Disney propuesto por alkemy",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon index.js",
    "migration:generate": "sequelize-cli migration:generate --name",
    "migration:run": "sequelize-cli db:migrate",
    "migration:revert": "sequelize-cli db:migrate:undo",
    "migration:clear": "sequelize-cli db:migrate:undo:all",
    "seed:generate": "sequelize-cli seed:generate --name",
    "seed:run": "npx sequelize-cli db:seed:all",
    "seed:revert": "npx sequelize-cli db:seed:undo",
    "seed:clear": "sequelize-cli db:seed:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlRazeto/Alkemy-Node.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AlRazeto/Alkemy-Node/issues"
  },
  "homepage": "https://github.com/AlRazeto/Alkemy-Node#readme",
  "dependencies": {
    "@hapi/boom": "^9.1.4",
    "@sendgrid/mail": "^7.6.1",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^2.0.15",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.15.0",
    "sequelize-cli": "^6.4.1"
  }
}
